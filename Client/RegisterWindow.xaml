<Window x:Class="Client.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="IpAddressTextBox" HorizontalAlignment="Center" Margin="0,114,0,0" TextWrapping="Wrap" Text="IP Address" VerticalAlignment="Top" Width="200" Height="37"/>
        <TextBox Name="PortTextBox" HorizontalAlignment="Center" Margin="0,180,0,0" TextWrapping="Wrap" Text="Port" VerticalAlignment="Top" Width="200" Height="37"/>
        <Button Name="RegisterButton" Content="Register" HorizontalAlignment="Center" Margin="0,277,0,0" VerticalAlignment="Top" Height="58" Width="92" Click="RegisterButton_Click"/>

    </Grid>
</Window>
