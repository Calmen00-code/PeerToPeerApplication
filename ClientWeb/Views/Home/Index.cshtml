@{
    @using ClientWeb.Models;
    @model IEnumerable<Peer>;
}
<div id="tableview">
    <table>
        <tr>
            <td>IP Address</td>
            <td>Port</td>
            <td>Idle Status</td>
            <td>Completed Job Number</td>
        </tr>
        @foreach (var peer in Model)
        {
            <tr>
                <td>@peer.IP_Address</td>
                <td>@peer.Port</td>
                <td>@peer.Idle</td>
                <td>@peer.CompletedJob</td>
            </tr>
        }
    </table>

</div>
